import tkinter as tk
from module3 import Module3

class MonOilStudyApp(tk.Tk):
    def __init__(self):
        super().__init__()
        self.title("MonOilStudy Test")
        self.geometry("800x600")
        
        # Инициализация модулей
        self.module3 = Module3(self)
        self.module3.pack(fill="both", expand=True)
        
        # Другие модули и логика программы
        # ...

if __name__ == "__main__":
    app = MonOilStudyApp()
    app.mainloop() 